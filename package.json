{
    "name": "manual-image-rec",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "cross-env NODE_ENV=development node ./bin/start-server.js",
        "build": "npm run clean && babel src --out-dir lib --ignore static --source-maps inline",
        "build-prod": "npm run clean && webpack -p && babel src --out-dir lib --ignore static",
        "clean": "shx rm -rf lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uavaustin/manual-image-rec.git"
    },
    "author": "UAV Austin <uavaustin@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/uavaustin/manual-image-rec/issues"
    },
    "main": "lib/server.js",
    "dependencies": {
        "express": "^4.16.2",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
    },
    "devDependencies": {
        "autoprefixer": "^7.1.6",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "copy-webpack-plugin": "^4.2.0",
        "cross-env": "^5.1.3",
        "css-loader": "^0.28.7",
        "extract-text-webpack-plugin": "^3.0.2",
        "json-loader": "^0.5.7",
        "node-sass": "^4.5.3",
        "postcss-loader": "^2.0.8",
        "sass-loader": "^6.0.6",
        "shx": "^0.2.2",
        "source-map-support": "^0.5.3",
        "webpack": "^3.8.1",
        "webpack-dev-middleware": "^2.0.4"
    },
    "babel": {
        "presets": [
            "env",
            "react"
        ],
        "plugins": [
            "add-module-exports"
        ]
    }
}
